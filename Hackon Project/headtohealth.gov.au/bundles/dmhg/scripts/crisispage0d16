(function(){function n(n){t(n);$(".container .state-container").removeClass("show").addClass("hidden").attr("tabindex",-1);$(".container .state-container."+n.text()).removeClass("hidden").addClass("show").attr("tabindex",0).focus();$(".no-state-selected-message").hide()}function t(n){i(n);r(n)}function i(n){n.children().is(":button")&&(n.parent().children().each(function(){$(this).children().attr("aria-selected","false")}),n.children().attr("aria-selected","true"))}function r(n){$(".helpline-state-menu.dropdown").hasClass("open")&&($(".helpline-state-menu.dropdown").removeClass("open"),$("#dropdownStateMenu").attr("aria-expanded",!1).text(n.text()))}$(document).ready(function(){$(".helpline-state-menu li").on("click",function(){n($(this))});$(".helpline-state-menu li").on("keydown",function(t){(t.which==Constants.KeyCode.Enter||t.which==Constants.KeyCode.Space)&&n($(this))})})})(window.CrisisPage=window.CrisisPage||{});