(function(){var n=$("#resource-page-card");$(document).ready(function(){ContentCards.showModalForCard(n,n);var t=$("h1"),i=$('h2.title[aria-hidden="true"]');t.text(t.text().replace("Resources","Resource")+" - "+i.text());$(".escape-to-close-this").remove();SocialBar.init({desktopContainer:"#social-bar-desktop-container",mobileContainer:"#social-bar-mobile-container"});$("#feedback-tab").css("bottom","295px");setTimeout(function(){var n=$("h2.title.sr-only").text()+" | "+$("div.ssk.ssk-group").attr("data-text");$("div.ssk.ssk-group").each(function(){$(this).attr("data-url",location.href);$(this).attr("data-text",n)});$("#social-bar-desktop-container").find("h3").replaceWith(function(){return"<h2 class='heading SocialBar'>"+$(this).text()+"<\/h2>"})},1200)})})();